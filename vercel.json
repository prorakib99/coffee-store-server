{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/coffees",
        "dest": "/index.js"
      },
      {
        "src": "/coffees/:id",
        "dest": "/index.js"
      },
      {
        "src": "/",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  